package patterns;

public class pascaleTriangle {
    int row ,col;
    int[][] arr=0;
//	int arr[row][col]=0;
	for( row= 1 ; row<=5 ; row++) {
		for(col = 1; col<=row ;col ++) {
			
			if(col == 1 || col == row) {
				
				arr[col][row] = 1;
				System.out.println( arr[col][row]);
			}
			else {
				arr[row][col] = arr[row-1][col-1] + arr[row -1][col];
			}
		}
	}
}
